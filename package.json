{
  "name": "@tableside/restaurant",
  "version": "0.1.0",
  "description": "A microservice component for manging restaurants.",
  "main": "dist/server.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Table-Side/Restaurant"
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "yarn prisma:gen && yarn tsc --project ./",
    "prisma:gen": "prisma generate",
    "prisma:migrate:initial": "mkdir -p prisma/migrations/0_init && prisma migrate diff --from-empty --to-schema-datamodel ./prisma/schema.prisma --script > ./prisma/migrations/0_init/migration.sql",
    "prisma:migrate:create": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "start": "node dist/server.js",
    "start:migrate": "yarn prisma:migrate:deploy && yarn start",
    "start:dev": "nodemon src/server.ts",
    "start:migrate:dev": "yarn prisma:migrate:create && yarn start:dev",
    "test": "echo \"Error: no test specified\""
  },
  "dependencies": {
    "@prisma/client": "^5.11.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.20",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^4.0.1",
    "prisma": "^5.10.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
